        tools:layout_editor_absoluteY="0dp" />

    <EditText
        android:id="@+id/editText17"
        android:layout_width="225dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="CPF ou Nome de UsuÃ¡rio"
